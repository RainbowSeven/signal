{
  "manifest_version": 2,

  "name": "Signal",
  "description": "Schedule a URL to open at a future time ",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Signal",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "alarms",
    "https://ajax.googleapis.com/"
  ]
}